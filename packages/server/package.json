{
  "name": "@thepineappledev/bot-server",
  "version": "0.1.2",
  "description": "Api for discord bot",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --expose-gc src/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "build": "npx tsc --p ./tsconfig.build.json",
    "start": "tsc && node dist/index.js",
    "deploy": "node --max-old-space-size=200 --expose-gc dist/index.js "
  },
  "private": true,
  "author": "J. van der Tang",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^0.6.0",
    "@discordjs/opus": "^0.8.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@discordjs/voice": "^0.15.0",
    "@rollup/plugin-json": "^5.0.1",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "busboy": "^1.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "discord-api-types": "^0.23.1",
    "discord.js": "^13.12.0",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-sse": "^0.5.3",
    "ffmpeg-static": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "kill-port": "^1.6.1",
    "libsodium-wrappers": "^0.7.9",
    "lodash": "^4.17.21",
    "mongodb": "^3.7.1",
    "mongoose": "^5.13.9",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.13",
    "play-dl": "^1.9.6",
    "rollup": "^3.4.0",
    "ts-node": "^10.9.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/filesystem": "^0.0.32",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.11.7",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "discord.js-typings": "github:discordjs/discord.js-typings",
    "eslint": "^7.32.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.13",
    "rollup-plugin-dts": "^5.0.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  }
}
